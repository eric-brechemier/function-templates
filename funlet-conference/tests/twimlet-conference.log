[CONFERENCE-1-1] Request Password: No Password Provided
<?xml version="1.0" encoding="UTF-8"?>
<Response>
  <Gather numDigits="4">
    <Say>Please enter your conference pass code</Say>
  </Gather>
  <Redirect/>
</Response>

[CONFERENCE-1-2] Request Password: Wrong Password Provided
<?xml version="1.0" encoding="UTF-8"?>
<Response>
  <Gather numDigits="4">
    <Say>Please enter your conference pass code</Say>
  </Gather>
  <Redirect/>
</Response>

[CONFERENCE-2-1] Correct Password
<?xml version="1.0" encoding="UTF-8"?>
<Response>
  <Say>You are now entering the conference line.</Say>
  <Dial>
    <Conference>d41d8cd98f00b204e9800998ecf8427e</Conference>
  </Dial>
</Response>

[CONFERENCE-2-2] Named Conference (from example 1)
<?xml version="1.0" encoding="UTF-8"?>
<Response>
  <Say>You are now entering the conference line.</Say>
  <Dial>
    <Conference>foo</Conference>
  </Dial>
</Response>

[CONFERENCE-2-3] Text Message
<?xml version="1.0" encoding="UTF-8"?>
<Response>
  <Say>Custom message</Say>
  <Dial>
    <Conference>foo</Conference>
  </Dial>
</Response>

[CONFERENCE-2-4] Recorded Message
<?xml version="1.0" encoding="UTF-8"?>
<Response>
  <Play>https://example.com/recorded-message.mp3</Play>
  <Dial>
    <Conference>foo</Conference>
  </Dial>
</Response>

[CONFERENCE-2-5] Hold Music from Twilio Genres (from example 3)
<?xml version="1.0" encoding="UTF-8"?>
<Response>
  <Say>You are now entering the conference line.</Say>
  <Dial>
    <Conference waitUrl="http://twimlets.com/holdmusic?Bucket=com.twilio.music.rock" waitMethod="GET">d41d8cd98f00b204e9800998ecf8427e</Conference>
  </Dial>
</Response>

